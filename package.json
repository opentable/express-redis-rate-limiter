{
  "name": "redis-express-rate-limiter",
  "version": "1.0.2",
  "description": "An redis and express based rate limiter to use on API endpoints to limit the number of requests based on custom logic for a set duration",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "nyc ava",
    "test-watch": "npm test -- --watch",
    "build": "babel lib/redis-express-rate-limiter.js --out-file lib/index.js",
    "prepublish": "npm run build",
    "coverage": "nyc ava report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opentable/redis-express-rate-limiter"
  },
  "bugs": {
    "url": "https://github.com/opentable/redis-express-rate-limiter/issues"
  },
  "keywords": [
    "express",
    "redis",
    "rate",
    "limit",
    "limter",
    "rate-limiter",
    "brute-force",
    "bruteforce",
    "defense",
    "throttle",
    "security"
  ],
  "author": "Ivy Rogatko",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "body-parser": "^1.15.2",
    "coveralls": "^2.11.14",
    "faker": "^3.1.0",
    "nyc": "^8.4.0",
    "redis-js": "^0.1.2",
    "sinon": "^1.17.6",
    "supertest": "^2.0.1"
  },
  "dependencies": {
    "express": "^4.14.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
